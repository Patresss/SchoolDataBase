<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<ScrollPane>
		<content>
			<VBox>
				<children>
					<VBox>
						<children>
							<Label styleClass="title-label" text="%list.schoolStaff.header" />
							<VBox fx:id="staffSchoolVBox" />
						</children>
					</VBox>
					<VBox>
						<children>

							<Label styleClass="title-label" text="%list.teacher.header" />
							<VBox fx:id="teacherVBox" />
						</children>
					</VBox>
				</children>
			</VBox>
		</content>
		<opaqueInsets>
			<Insets />
		</opaqueInsets>
		<StackPane.margin>
			<Insets left="50.0"/>
		</StackPane.margin>
	</ScrollPane>
</StackPane>
