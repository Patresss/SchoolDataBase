<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import com.jfoenix.*?>
<?import com.jfoenix.controls.*?>

<StackPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<BorderPane>
		<top>
			<Label styleClass="label-title" text="%statistic.header" BorderPane.alignment="TOP_LEFT" />
		</top>
      <center>
         <GridPane hgap="10.0" styleClass="grid-pane-statistic">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="280.0" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Label text="%statistic.staff_per_subject" />
               <Label text="%statistic.staff_per_duty" GridPane.rowIndex="1" />
               <Label fx:id="staffPerSubjectLabel" text="Label" GridPane.columnIndex="1" />
               <Label fx:id="staffPerDutyLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            </children>
            <BorderPane.margin>
               <Insets left="50.0" top="50.0" />
            </BorderPane.margin>
         </GridPane>
      </center>
	</BorderPane>
</StackPane>
